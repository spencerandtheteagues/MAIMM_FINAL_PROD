modules = ["nodejs-20", "python-3.11", "postgresql-16"]
run = "npm run start"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-25_05"
packages = [
  "freetype",
  "jq",
  "lcms2",
  "libimagequant",
  "libjpeg",
  "libtiff",
  "libwebp",
  "libxcrypt",
  "openjpeg",
  "tcl",
  "tk",
  "zip",
  "zlib",
  "ffmpeg",
]

[deployment]
deploymentTarget = "autoscale"
build = ["sh", "-c", "npm run build"]
run = ["sh", "-c", "npm run start"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33083
externalPort = 3003

[[ports]]
localPort = 37367
externalPort = 3000

[[ports]]
localPort = 41787
externalPort = 3001

[[ports]]
localPort = 42413
externalPort = 4200

[[ports]]
localPort = 46091
externalPort = 3002

[env]
PORT = "5000"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run start"
waitForPort = 5000

[agent]
integrations = ["javascript_openai:1.0.0", "javascript_gemini:1.0.0", "javascript_database:1.0.0", "javascript_log_in_with_replit:1.0.0"]
